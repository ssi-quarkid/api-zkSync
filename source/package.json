{
    "private": true,
    "name": "extrimian-sidetree",
    "version": "1.0.0",
    "workspaces": [
      "packages/common",
      "packages/cas",
      "packages/db",
      "packages/ledger",
      "packages/cas-ipfs",
      "packages/core",
      "packages/did-method",
      "packages/ledger-ethereum",
      "packages/ledger-rsk",
      "packages/ledger-zksync",
      "packages/did-method-modena",
      "packages/did-method-modena-api"
    ],
  "scripts": {
    "start": "cd packages/did-method-modena-api/ && npm run start",
    "build": "yarn workspaces run build",
    "clean": "find . \\( -name 'node_modules' -o -name 'dist' -o -name 'lib' \\) -type d -prune -exec rm -rf '{}' +",
    "clean:lock": "npx lerna exec 'rm -rf package-lock.json node_modules' --stream",
    "postinstall": "yarn workspaces run build",
    "install:clean": " npx lerna clean -y && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "yarn": "1.22.19 ",
    "@babel/types": "7.22.10"
  }
}
